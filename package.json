{
  "name": "tienda-online-mvc",
  "version": "1.0.0",
  "description": "Tienda online con arquitectura MVC y Redux",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mvc": "VITE_USE_REDUX=false vite",
    "dev:redux": "VITE_USE_REDUX=true vite",
    "build": "vite build",
    "build:mvc": "VITE_USE_REDUX=false vite build",
    "build:redux": "VITE_USE_REDUX=true vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "serve": "npm run build && npm run preview"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mvc",
    "redux",
    "typescript",
    "pwa",
    "ecommerce",
    "vite"
  ],
  "author": "Tu nombre",
  "license": "MIT"
}
